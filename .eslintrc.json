{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "parser": "babel-eslint",
  "rules": {
    "no-unused-expressions": 0,
    "babel/no-unused-expressions": 1,
    "no-useless-escape": 0
  },
  "plugins": ["babel"]
}
